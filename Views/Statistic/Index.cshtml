
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<div class="alert alert-danger">Kategori Sayısı: @ViewBag.categoryCount</div>
<div class="alert alert-warning">Proje Sayısı: @ViewBag.projectCount</div>
<div class="alert alert-success">Yetenek Sayısı: @ViewBag.skillCount</div>
<div class="alert alert-info">Yetenek Ortalaması: @ViewBag.skillAvgValue</div>
<div class="alert alert-primary">Son Eklenen Yetenek: @ViewBag.lastSkillTitleName</div>
<div class="alert alert-secondary">Son Eklenen Hizmet: @ViewBag.lastServiceTitleName</div>
<div class="alert alert-info">Asp .Net Core Proje Sayısı: @ViewBag.coreCategoryProjectCount</div>
<div class="alert alert-danger">İlk Kategori: @ViewBag.CategoryFirst</div>
<div class="alert alert-dark">İlk Proje: @ViewBag.FirstProject</div>
<div class="alert alert-info">Sosyal Medya Sayısı: @ViewBag.SocialCount</div>

